<div id="App">
	<section id="MainNav">
		<img src="assets/images/pf_logo.png">
		<div id="MainNavCarbon">
			{{bs-pills contentBinding="carbonNav" selectedBinding="selected" stacked=true}}
		</div>
		<div id="MainNavLegacy">
			<a {{action 'flexTo' 'eRx'}}>eRx</a>
			<a {{action 'flexTo' 'Labs/Imaging'}}>Labs/Imaging</a>
			<a {{action 'flexTo' 'Documents'}}>Documents</a>
		</div>
	</section>
	<section id="MainContent">
		<div id="UtilityNav">

			<div class="btn-group">
				<button type="button" class="btn dropdown-toggle" data-toggle="dropdown"><i class="fa fa-question-circle"></i>&nbsp;&nbsp;Help&nbsp;&nbsp;<i class="fa fa-angle-down fa-lg"></i></button>
				<ul class="dropdown-menu">
					<li><a>Help Forum</a></li>
					<li><a>Learning Center</a></li>
					<li><a>Live Chat</a></li>
					<li><a>Contact Us</a></li>
					<li><a>Privacy Policy</a></li>
					<li><a>Terms of Use</a></li>
				</ul>
			</div>

			<div class="btn-group">
				<button type="button" class="btn dropdown-toggle user-menu" data-toggle="dropdown">Dr. StrangeLove | StrangeLove Practice&nbsp;&nbsp;<i class="fa fa-angle-down fa-lg"></i></button>
				<ul class="dropdown-menu">
					<li><a>Dropdown link</a></li>
					<li><a>Dropdown link</a></li>
				</ul>
			</div>

			{{!-- <a class="user_info">Dr. StrangeLove | StrangeLove Practice &nbsp; <i class="fa fa-chevron-down"></i></a> --}}
			{{bs-pills contentBinding="utilityNav" selectedBinding="selected"}}
			<a {{action 'logout'}}>Logout</a>
		</div>
		<section id="Content">{{outlet}}</section>
		<section id="BannerBottom">
			<div class="banner"></div>
		</section>
	</section>
	<section id="BannerRight">
		<div class="banner_small"></div>
		<div class="banner_large"></div>
	</section>
</div>